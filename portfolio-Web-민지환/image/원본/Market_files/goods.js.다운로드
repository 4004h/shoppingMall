let json = JSON.parse(JSON.stringify(data));

$(document).ready(function () {
    let url = new URLSearchParams(location.search);
    let id = url.get("id")

    if(id != '')
    {
        console.log("ì—†ìŒ")
    }
    
    console.log(json[json.findIndex(itme => itme.id == id)].img)
    
    $("#goodsPage img").attr("src", `.${json[json.findIndex(itme => itme.id == id)].img}`);
    

})


















